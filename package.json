{
  "name": "kirocore",
  "version": "0.1.0",
  "description": "Universal AI agent platform with 2 separate applications: StudyBuddy (AI tutor) and IdeaForge (creative partner). Built for Skeleton Crew hackathon.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "node server/index.js",
    "build": "next build",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "next start",
    "start:backend": "node server/index.js",
    "lint": "eslint"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "framer-motion": "^12.23.24",
    "next": "16.0.2",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
